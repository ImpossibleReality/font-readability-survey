{
  "name": "font-readability-survey",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync && npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@material/typography": "^13.0.0",
    "@smui/button": "^6.0.0-beta.15",
    "@smui/card": "^6.0.0-beta.15",
    "@smui/data-table": "^6.0.0-beta.16",
    "@smui/dialog": "^6.0.0-beta.16",
    "@smui/fab": "^6.0.0-beta.16",
    "@smui/linear-progress": "^6.0.0-beta.16",
    "@smui/paper": "^6.0.0-beta.15",
    "@smui/slider": "^6.0.0-beta.16",
    "@smui/tab": "^6.0.0-beta.16",
    "@smui/tab-bar": "^6.0.0-beta.16",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "smui-theme": "^6.0.0-beta.15",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module",
  "dependencies": {
    "@smui/form-field": "^6.0.0-beta.16",
    "@smui/radio": "^6.0.0-beta.16",
    "ajv": "^8.11.0",
    "cookie": "^0.5.0"
  }
}
